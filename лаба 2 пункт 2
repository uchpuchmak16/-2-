/*обычный индексовый цикл*/
#include <iostream>
#include <string>

int main() {
    std::string names[] = {
        "Иволгин Роман",
        "Петров Николай",
        "Лебедева Мария",
        "Вологодская Елена",
        "Семенов Андрей",
        "Тимощук Валентин",
        "Окладов Сергей",
        "Яковлев Лев",
        "Балконская Юлия",
        "Ахматова Анастасия"
    };

    int grades[] = {39, 89, 90, 78, 90, 100, 65, 34, 91, 100};

    for (size_t i = 0; i < sizeof(names) / sizeof(*names); i++) {
        std::cout << names[i] << " — " << grades[i] << "\n";
    }

    return 0;
}
